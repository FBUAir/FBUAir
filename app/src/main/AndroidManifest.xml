        <activity android:name="me.gnahum12345.fbuair.activities.MainActivity">
        </activity>
        <activity android:name="me.gnahum12345.fbuair.activities.DiscoverActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
